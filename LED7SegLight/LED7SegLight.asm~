	LEDP00	BIT	P0.0
	LEDP01	BIT	P0.1
	LEDP02	BIT	P0.2
	LEDP03	BIT	P0.3
	LEDP04	BIT	P0.4
	LEDP05	BIT	P0.5
	LEDP06	BIT	P0.6
	LEDP07	BIT	P0.7
	LEDP10	BIT	P1.0
	LEDP11	BIT	P1.1
	LEDP12	BIT	P1.2
	LEDP13	BIT	P1.3
	LEDP14	BIT	P1.4
	LEDP15	BIT	P1.5
	LEDP16	BIT	P1.6
	LEDP17	BIT	P1.7
	LEDP20	BIT	P2.0
	LEDP21	BIT	P2.1
	LEDP22	BIT	P2.2
	LEDP23	BIT	P2.3
	LEDP24	BIT	P2.4
	LEDP25	BIT	P2.5
	LEDP26	BIT	P2.6
	LEDP27	BIT	P2.7
	LEDP30	BIT	P3.0
	LEDP31	BIT	P3.1
	LEDP32	BIT	P3.2
	LEDP33	BIT	P3.3
	LEDP34	BIT	P3.4
	LEDP35	BIT	P3.5
	LEDP36	BIT	P3.6
	LEDP37	BIT	P3.7
	
	START	CODE	00H
	TIME	EQU	2
	
	;start testing code
	OUTLOOP	SET	05H
	INLOOP	SET	04H
	;end testing code

	ORG	START

BLINK:	
	CPL	LEDP00
	CPL	LEDP01
	CPL	LEDP02
	CPL	LEDP03
	CPL	LEDP04
	CPL	LEDP05
	CPL	LEDP06
	CPL	LEDP07
	MOV	R3,#TIME
	MOV	R2,#INLOOP
	MOV	R1,#OUTLOOP

LOOP:	
	DJNZ	R1,LOOP
	CPL	LEDP10
	CPL	LEDP11
	CPL	LEDP12
	CPL	LEDP13
	CPL	LEDP14
	CPL	LEDP15
	CPL	LEDP16
	CPL	LEDP17
	MOV	R1,#OUTLOOP
	
	DJNZ	R2,LOOP
	CPL	LEDP20
	CPL	LEDP21
	CPL	LEDP22
	CPL	LEDP23
	CPL	LEDP24
	CPL	LEDP25
	CPL	LEDP26
	CPL	LEDP27
	MOV	R2,#INLOOP
	
	DJNZ	R3,LOOP
	CPL	LEDP30
	CPL	LEDP31
	CPL	LEDP32
	CPL	LEDP33
	CPL	LEDP34
	CPL	LEDP35
	CPL	LEDP36
	CPL	LEDP37
	MOV	R3,#TIME
	
	SJMP	BLINK

	END